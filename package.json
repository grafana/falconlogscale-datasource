{
  "name": "grafana-falconlogscale-datasource",
  "version": "1.9.0",
  "description": "Falcon LogScale data source plugin for Grafana",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "e2e:playwright": "yarn playwright test",
    "e2e:playwright:ui": "yarn playwright test --ui",
    "e2e:playwright:report": "yarn playwright show-report",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\"",
    "start": "yarn watch",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "e2e": "playwright test"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@grafana/e2e": "11.0.4",
    "@grafana/e2e-selectors": "11.6.0",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/plugin-e2e": "^1.19.3",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.57.0",
    "@stylistic/eslint-plugin-ts": "^3.1.0",
    "@swc/core": "^1.15.3",
    "@swc/helpers": "^0.5.7",
    "@swc/jest": "^0.2.36",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.8.7",
    "@types/react-router-dom": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.50.0",
    "copy-webpack-plugin": "^12.0.2",
    "cspell": "^9.4.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-webpack-plugin": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^10.3.10",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-extended": "^3.0.7",
    "prettier": "^3.7.4",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.97.1",
    "sass-loader": "14.2.1",
    "style-loader": "3.3.4",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.15",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.9.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@grafana/data": "^11.6.0",
    "@grafana/plugin-ui": "^0.12.0",
    "@grafana/runtime": "^11.6.0",
    "@grafana/schema": "^11.3.2",
    "@grafana/ui": "^11.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "5.3.4",
    "react-select-event": "^5.5.1",
    "react-use": "^17.5.1",
    "tslib": "2.8.1"
  },
  "resolutions": {
    "rxjs": "7.8.2"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.22",
  "volta": {
    "node": "20.18.1",
    "yarn": "1.22.22"
  }
}
